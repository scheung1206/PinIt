<div id="newRoom">
  <fieldset>
  <legend>Start a New Room</legend>
<%= form_for(@new_room) do |f| %>
  Room Name: <%= f.text_field :name %><br/><br/>
  Public? <%= f.check_box :public %><br/><br/>

  <%= f.submit %>
<% end %>
</fieldset>
</div>
<br>
<fieldset>
  <legend>Existing Rooms</legend>
  <table cellpadding="2" cellspacing="5">
    <tr>
      <th>Room Name </th>
      <th>Started</th>
    </tr>

    <% @rooms.each do |room| %>
      <tr>
        <td width="300px"><a href="/rooms/<%= room.id %>"><%= room.name %></a></td>
        <td><%= room.created_at %></td>
      </tr>
    <% end %>
  </table>
</fieldset>
